{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npx nuxi build",
    "dev:local": "npx nuxi dev --host --dotenv ./environments/.env.local",
    "dev:staging": "npx nuxi dev --host --dotenv ./environments/.env.staging",
    "dev:production": "npx nuxi dev --host --dotenv ./environments/.env.production",
    "generate:local": "npx nuxi generate --dotenv ./environments/.env.local",
    "generate:staging": "npx nuxi generate --dotenv ./environments/.env.staging",
    "generate:production": "npx nuxi generate --dotenv ./environments/.env.production",
    "preview": "npx nuxi preview",
    "postinstall": "npx nuxi prepare",
    "lint:js": "eslint --ext \".ts,.vue\" --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lintfix": "prettier --write --list-different . && npm run lint:js --fix"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/i18n": "^8.0.0-rc.5",
    "@nuxtjs/moment": "^1.6.1",
    "@types/crypto-js": "^4.1.3",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nuxt": "^3.8.0",
    "nuxt-primevue": "^0.0.3",
    "prettier": "^3.0.3",
    "sass": "^1.68.0",
    "typescript": "^5.2.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.1",
    "crypto-js": "^4.1.1",
    "pinia": "^2.1.7",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.37.0",
    "vee-validate": "^4.11.8",
    "vue3-otp-input": "^0.4.1",
    "yup": "^1.3.2"
  }
}
